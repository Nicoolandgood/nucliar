{
  "name": "nucliar",
  "version": "0.1.1",
  "description": "Command-line tool to generate React components",
  "author": "Teaflex",
  "license": "MIT",
  "homepage": "https://github.com/Nicoolandgood/nucliar#readme",
  "keywords": [
    "react",
    "cli",
    "tool",
    "generator",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Nicoolandgood/nucliar.git"
  },
  "bugs": {
    "url": "https://github.com/Nicoolandgood/nucliar/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "nucliar": "./bin/nucliar"
  },
  "scripts": {
    "start": "node ./dist",
    "build": "rm -rf ./dist && tsup",
    "run:dev": "ts-node ./src/cli.ts",
    "test": "jest",
    "lint": "tsc",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.196",
    "@types/node": "^20.4.2",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^11.0.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "enquirer": "^2.3.6",
    "lodash": "^4.17.21",
    "squirrelly": "^9.0.0"
  }
}
